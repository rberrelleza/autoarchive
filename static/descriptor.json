{
  "key": "hiparchiver",
  "name": "Auto Archiver",
  "description": "Automatically archive your rooms after they have bee idle for a while.",
  "vendor": {
      "name": "hipchat",
      "url": "http://hipchat.com"
  },
  "links": {
      "self": "{{.BaseURL}}/descriptor",
      "homepage": "{{.BaseURL}}/descriptor"
  },
  "capabilities": {
    "hipchatApiConsumer": {
      "avatar": "{{.BaseURL}}/archiver.png",
      "scopes": [
          "view_group",
          "manage_rooms",
          "admin_room",
          "send_notification"
      ]
    },
    "installable": {
      "allowGlobal": true,
      "allowRoom": false,
      "callbackUrl": "{{.BaseURL}}/installable"
    },
    "configurable": {
        "url": "{{.BaseURL}}/configurable"
    }
  }
}
